{
  "name": "AI Research Bot - Real-time Knowledge Processing",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "knowledge-bot-webhook",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Knowledge Bot Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1.1,
      "position": [240, 300],
      "webhookId": "knowledge-bot-webhook"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "={{ $json.confidence_score }}",
            "operation": "largerEqual",
            "rightValue": 0.9
          }
        }
      },
      "id": "high-confidence-check",
      "name": "High Confidence?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [460, 300]
    },
    {
      "parameters": {
        "url": "=http://your-research-bot-url/api/n8n/knowledge-pairs/{{ $json.id }}",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "requestMethod": "PUT",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "validated",
              "value": true
            }
          ]
        }
      },
      "id": "auto-validate",
      "name": "Auto-Validate Knowledge Pair",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [680, 200]
    },
    {
      "parameters": {
        "jsCode": "// Analyze knowledge pair for quality indicators\nconst knowledgePair = $input.first().json;\n\n// Quality scoring logic\nlet qualityScore = 0;\n\n// Check problem clarity (length, question words)\nif (knowledgePair.problem.length > 20 && knowledgePair.problem.length < 200) {\n  qualityScore += 0.3;\n}\nif (/\\b(how|what|why|when|where|which)\\b/i.test(knowledgePair.problem)) {\n  qualityScore += 0.2;\n}\n\n// Check solution completeness\nif (knowledgePair.solution.length > 50) {\n  qualityScore += 0.3;\n}\nif (/\\b(use|implement|try|install|configure)\\b/i.test(knowledgePair.solution)) {\n  qualityScore += 0.2;\n}\n\nreturn [{\n  ...knowledgePair,\n  quality_score: qualityScore,\n  needs_review: qualityScore < 0.6\n}];"
      },
      "id": "quality-analysis",
      "name": "Analyze Quality",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [680, 400]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "={{ $json.needs_review }}",
            "operation": "equal",
            "rightValue": true
          }
        }
      },
      "id": "needs-review-check",
      "name": "Needs Review?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [900, 400]
    },
    {
      "parameters": {
        "channel": "#ai-research-bot",
        "text": "🔍 New knowledge pair needs review:\\n\\n*Problem:* {{ $json.problem }}\\n*Solution:* {{ $json.solution }}\\n*Confidence:* {{ $json.confidence_score }}\\n*Quality Score:* {{ $json.quality_score }}\\n\\n<http://your-research-bot-url/knowledge-pairs|Review in Dashboard>",
        "otherOptions": {}
      },
      "id": "review-notification",
      "name": "Send Review Alert",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 2.1,
      "position": [1120, 400]
    },
    {
      "parameters": {
        "channel": "#ai-research-bot",
        "text": "✅ High-quality knowledge pair auto-validated:\\n\\n*Problem:* {{ $('Auto-Validate Knowledge Pair').item.json.problem }}\\n*Solution:* {{ $('Auto-Validate Knowledge Pair').item.json.solution }}\\n*Confidence:* {{ $('Auto-Validate Knowledge Pair').item.json.confidence_score }}",
        "otherOptions": {}
      },
      "id": "success-notification",
      "name": "Send Success Alert",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 2.1,
      "position": [900, 200]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={ \"status\": \"processed\", \"action\": \"{{ $('High Confidence?').item.json ? 'auto-validated' : 'queued-for-review' }}\", \"quality_score\": \"{{ $json.quality_score }}\" }"
      },
      "id": "webhook-response",
      "name": "Webhook Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1340, 300]
    }
  ],
